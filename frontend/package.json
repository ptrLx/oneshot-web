    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^22.1.0",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "3.2.4",
    "typescript": "^5.1.6",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^0.16.6",
    "vitest": "^0.32.2",
