    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@ionic/pwa-elements": "^3.2.2",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.5.1",
    "form-data": "^4.0.0",
    "ionicons": "^7.0.0",
    "pinia": "^2.1.7",
    "swiper": "^10.3.1",
